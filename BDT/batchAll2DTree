#!/bin/bash

#SBATCH -p largemem
#SBATCH -N 1
#SBATCH -n 1
#SBATCH -t 0-6:00
#SBATCH --mem 5000
#SBATCH --mail-type=END,FAIL                                         
#SBATCH --mail-user=ben.nissan@tufts.edu                 

sbatch run2DTree LinearHardTraining.arff LinearHardTestingAll.arff Results/LinearHardTestingAll0520
sbatch run2DTree LinearHardTraining.arff LinearHardTestingSignal.arff Results/LinearHardTestingSignal0520
sbatch run2DTree LinearHardTraining.arff LinearHardTestingBackground.arff Results/LinearHardTestingBackground0520

sbatch run2DTree LinearSoftTraining.arff LinearSoftTestingAll.arff Results/LinearSoftTestingAll0520
sbatch run2DTree LinearSoftTraining.arff LinearSoftTestingSignal.arff Results/LinearSoftTestingSignal0520
sbatch run2DTree LinearSoftTraining.arff LinearSoftTestingBackground.arff Results/LinearSoftTestingBackground0520


sbatch run2DTree CircleHardTraining.arff CircleHardTestingAll.arff Results/CircleHardTestingAll0520
sbatch run2DTree CircleHardTraining.arff CircleHardTestingSignal.arff Results/CircleHardTestingSignal0520
sbatch run2DTree CircleHardTraining.arff CircleHardTestingBackground.arff Results/CircleHardTestingBackground0520

sbatch run2DTree CircleSoftTraining.arff CircleSoftTestingAll.arff Results/CircleSoftTestingAll0520
sbatch run2DTree CircleSoftTraining.arff CircleSoftTestingSignal.arff Results/CircleSoftTestingSignal0520
sbatch run2DTree CircleSoftTraining.arff CircleSoftTestingBackground.arff Results/CircleSoftTestingBackground0520


sbatch run2DTree TwoCircleHardTraining.arff TwoCircleHardTestingAll.arff Results/TwoCircleHardTestingAll0520
sbatch run2DTree TwoCircleHardTraining.arff TwoCircleHardTestingSignal.arff Results/TwoCircleHardTestingSignal0520
sbatch run2DTree TwoCircleHardTraining.arff TwoCircleHardTestingBackground.arff Results/TwoCircleHardTestingBackground0520

sbatch run2DTree TwoCircleSoftTraining.arff TwoCircleSoftTestingAll.arff Results/TwoCircleSoftTestingAll0520
sbatch run2DTree TwoCircleSoftTraining.arff TwoCircleSoftTestingSignal.arff Results/TwoCircleSoftTestingSignal0520
sbatch run2DTree TwoCircleSoftTraining.arff TwoCircleSoftTestingBackground.arff Results/TwoCircleSoftTestingBackground0520