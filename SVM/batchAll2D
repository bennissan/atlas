#!/bin/bash
#SBATCH -p batch
#SBATCH --constraint=M3
#SBATCH --ntasks-per-core=1
#SBATCH --ntasks=1
#SBATCH --output=my_batch.%N.%J.out
#SBATCH --error=my_batch.%N.%J.err
#SBATCH --mail-type=END,FAIL
#SBATCH --mail-user=bnissa01@tufts.edu

sbatch -c 4 --time=4320 --mem=15000 run2D LinearHardTraining LinearHardTestingAll Results/LinearHardTestingAll0611
sbatch -c 4 --time=4320 --mem=15000 run2D LinearHardTraining LinearHardTestingSignal Results/LinearHardTestingSignal0611
sbatch -c 4 --time=4320 --mem=15000 run2D LinearHardTraining LinearHardTestingBackground Results/LinearHardTestingBackground0611

sbatch -c 4 --time=4320 --mem=15000 run2D LinearSoftTraining LinearSoftTestingAll Results/LinearSoftTestingAll0611
sbatch -c 4 --time=4320 --mem=15000 run2D LinearSoftTraining LinearSoftTestingSignal Results/LinearSoftTestingSignal0611
sbatch -c 4 --time=4320 --mem=15000 run2D LinearSoftTraining LinearSoftTestingBackground Results/LinearSoftTestingBackground0611


sbatch -c 4 --time=4320 --mem=15000 run2D CircleHardTraining CircleHardTestingAll Results/CircleHardTestingAll0611
sbatch -c 4 --time=4320 --mem=15000 run2D CircleHardTraining CircleHardTestingSignal Results/CircleHardTestingSignal0611
sbatch -c 4 --time=4320 --mem=15000 run2D CircleHardTraining CircleHardTestingBackground Results/CircleHardTestingBackground0611

sbatch -c 4 --time=4320 --mem=15000 run2D CircleSoftTraining CircleSoftTestingAll Results/CircleSoftTestingAll0611
sbatch -c 4 --time=4320 --mem=15000 run2D CircleSoftTraining CircleSoftTestingSignal Results/CircleSoftTestingSignal0611
sbatch -c 4 --time=4320 --mem=15000 run2D CircleSoftTraining CircleSoftTestingBackground Results/CircleSoftTestingBackground0611


sbatch -c 4 --time=4320 --mem=15000 run2D TwoCircleHardTraining TwoCircleHardTestingAll Results/TwoCircleHardTestingAll0611
sbatch -c 4 --time=4320 --mem=15000 run2D TwoCircleHardTraining TwoCircleHardTestingSignal Results/TwoCircleHardTestingSignal0611
sbatch -c 4 --time=4320 --mem=15000 run2D TwoCircleHardTraining TwoCircleHardTestingBackground Results/TwoCircleHardTestingBackground0611

sbatch -c 4 --time=4320 --mem=15000 run2D TwoCircleSoftTraining TwoCircleSoftTestingAll Results/TwoCircleSoftTestingAll0611
sbatch -c 4 --time=4320 --mem=15000 run2D TwoCircleSoftTraining TwoCircleSoftTestingSignal Results/TwoCircleSoftTestingSignal0611
sbatch -c 4 --time=4320 --mem=15000 run2D TwoCircleSoftTraining TwoCircleSoftTestingBackground Results/TwoCircleSoftTestingBackground0611
