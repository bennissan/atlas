#!/bin/bash
#SBATCH -p largemem
#SBATCH --constraint=M3
#SBATCH --ntasks-per-core=1
#SBATCH --ntasks=1
#SBATCH --output=my_batch.%N.%J.out
#SBATCH --error=my_batch.%N.%J.err
#SBATCH --mail-type=END,FAIL
#SBATCH --mail-user=bnissa01@tufts.edu

sbatch -c 4 --time=6480 --mem=20000 run2D LinearHardTraining LinearHardTestingAll Results/LinearHardTestingAll0705
sbatch -c 4 --time=6480 --mem=20000 run2D LinearHardTraining LinearHardTestingSignal Results/LinearHardTestingSignal0705
sbatch -c 4 --time=6480 --mem=20000 run2D LinearHardTraining LinearHardTestingBackground Results/LinearHardTestingBackground0705

sbatch -c 4 --time=6480 --mem=20000 run2D LinearSoftTraining LinearSoftTestingAll Results/LinearSoftTestingAll0705
sbatch -c 4 --time=6480 --mem=20000 run2D LinearSoftTraining LinearSoftTestingSignal Results/LinearSoftTestingSignal0705
sbatch -c 4 --time=6480 --mem=20000 run2D LinearSoftTraining LinearSoftTestingBackground Results/LinearSoftTestingBackground0705


sbatch -c 4 --time=6480 --mem=20000 run2D CircleHardTraining CircleHardTestingAll Results/CircleHardTestingAll0705
sbatch -c 4 --time=6480 --mem=20000 run2D CircleHardTraining CircleHardTestingSignal Results/CircleHardTestingSignal0705
sbatch -c 4 --time=6480 --mem=20000 run2D CircleHardTraining CircleHardTestingBackground Results/CircleHardTestingBackground0705

sbatch -c 4 --time=6480 --mem=20000 run2D CircleSoftTraining CircleSoftTestingAll Results/CircleSoftTestingAll0705
sbatch -c 4 --time=6480 --mem=20000 run2D CircleSoftTraining CircleSoftTestingSignal Results/CircleSoftTestingSignal0705
sbatch -c 4 --time=6480 --mem=20000 run2D CircleSoftTraining CircleSoftTestingBackground Results/CircleSoftTestingBackground0705


sbatch -c 4 --time=6480 --mem=20000 run2D TwoCircleHardTraining TwoCircleHardTestingAll Results/TwoCircleHardTestingAll0705
sbatch -c 4 --time=6480 --mem=20000 run2D TwoCircleHardTraining TwoCircleHardTestingSignal Results/TwoCircleHardTestingSignal0705
sbatch -c 4 --time=6480 --mem=20000 run2D TwoCircleHardTraining TwoCircleHardTestingBackground Results/TwoCircleHardTestingBackground0705

sbatch -c 4 --time=6480 --mem=20000 run2D TwoCircleSoftTraining TwoCircleSoftTestingAll Results/TwoCircleSoftTestingAll0705
sbatch -c 4 --time=6480 --mem=20000 run2D TwoCircleSoftTraining TwoCircleSoftTestingSignal Results/TwoCircleSoftTestingSignal0705
sbatch -c 4 --time=6480 --mem=20000 run2D TwoCircleSoftTraining TwoCircleSoftTestingBackground Results/TwoCircleSoftTestingBackground0705
